import java.util.Scanner;
class Revnum{
  public static void main(String[] args) {
int num;
int reversed = 0,num;
 Scanner sc = new Scanner(System.in);
      System.out.println("Number:");
      
while( num!= 0) {
    

      int digit = num % 10;
      reversed = reversed * 10 + digit;

      num /= 10;
    }

    System.out.println("Reversed Number: " + reversed);
  }
}
